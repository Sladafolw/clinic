@model IEnumerable<dynamic>
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<h4>Transfer To AnotherRoom</h4>
<hr />
@foreach (var item in Model)
{
    <div>
        <dl class="row">
            <dt class="col-sm-2">
                Date
            </dt>
            <dd class="col-sm-10">
                @Html.Raw( item.Date)
            </dd>


            <dt class="col-sm-2">
                Name
            </dt>
            <dd class="col-sm-10">
                @Html.Raw( item.Name)
            </dd>
            <dt class="col-sm-2">
                NumberHospitalRoom
            </dt>
            <dd class="col-sm-10">
                @Html.Raw( item.NumberHospitalRoom)
            </dd>
        </dl>
    </div>

    <div class="aaa">
        <div class="link">
            
            <div class="Details">  <a asp-action="Index">Back to List</a></div>
        </div>
    </div>
}
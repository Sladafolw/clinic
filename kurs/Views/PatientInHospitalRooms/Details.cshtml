@model IEnumerable<dynamic>

       <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>


@foreach (var item in Model)
{
    <div>
        <h4>PatientInHospitalRoom</h4>
        <hr />
        <dl class="row">

            <dt class="col-sm-2">
                Name
            </dt>
            <dd class="col-sm-10">
                @Html.Raw(item.Name)
            </dd>
            <dt class="col-sm-2">
                Age
            </dt>
            <dd class="col-sm-10">
                @Html.Raw(item.Age)
            </dd>
            <dt class="col-sm-2">
                NumberHospitalRoom
            </dt>
            <dd class="col-sm-10">
                @Html.Raw(item.NumberHospitalRoom)
            </dd>
        </dl>
    </div>
    <div class="aaa">
         <div class="link">
        <div class="Edit">   <a asp-action="Edit" asp-route-id="@item?.CodePatient">Edit</a> </div>|
       <div class="Details"><a asp-action="Index">Back to List</a></div> </div>
    </div>

}